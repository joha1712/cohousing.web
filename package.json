{
  "name": "mya.cohousing.website",
  "config": {
    "baseApiUrl": "https://localhost:44302/api"
  },
  "version": "0.1.0",
  "description": "Website for cohousing",
  "main": "index.js",
  "devDependencies": {
    "cross-env": "^6.0.3",
    "local-web-server": "^3.0.7",
    "replace-in-file": "^4.2.0",
    "shx": "^0.3.2"
  },
  "scripts": {
    "dev": "npm run build && ws",
    "build": "npm run clean && npm run package",
    "clean": "shx rm -rf dist",
    "package": "shx mkdir dist && shx cp -R LICENSE index.html config.js scripts styles dist && npm run updateConfig",
    "updateConfig": "cross-env-shell replace-in-file https://localhost:44302/api $npm_package_config_baseApiUrl dist/config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joha1712/cohousing.website.git"
  },
  "keywords": [
    "gymnastics",
    "video",
    "recorder"
  ],
  "author": "Jonas Thyregod",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/joha1712/cohousing.website/issues"
  },
  "homepage": "https://github.com/joha1712/cohousing.website#readme"  
}
